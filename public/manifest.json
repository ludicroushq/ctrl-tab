{
  "manifest_version": 2,
  "name": "tab",
  "version": "0.0.1",
  "description": "a new tab page",
  "short_name": "tab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "key": "",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "identity",
    "https://www.api.ludicrous.xyz/*",
    "https://todoist.com/oauth/*",
    "http://localhost:3000/*"
  ]
}
